{
  "expo": {
    "name": "ProjectLoloFrontEnd",
    "slug": "ProjectLoloFrontEnd",
    "version": "1.1.0",
    "orientation": "portrait",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.atieh.ProjectLoloFron",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app uses your library to upload photo's and video's",
        "NSCameraUsageDescription": "This app uses the camera to make video's"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "AUDIO_RECORDING",
        "CAMERA_ROLL"
      ]
    }
  }
}
